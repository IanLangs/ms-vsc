{
  "name": "mathscript-vscode",
  "displayName": "MathScript",
  "description": "Extensi√≥n completa de MathScript para VSCode",
  "version": "1.1.0",
  "engines": { "vscode": "^1.80.0" },
  "categories": ["Programming Languages"],
  "activationEvents": ["onLanguage:mathscript"],
  "main": "./extension.js",
  "icon": "MS.ico",
  "contributes": {
    "languages": [
      {
        "id": "mathscript",
        "aliases": ["MathScript", "ms"],
        "extensions": [".ms"],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "mathscript",
        "scopeName": "source.mathscript",
        "path": "./syntaxes/mathscript.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "mathscript",
        "path": "./snippets/snippets.json"
      }
    ],
    "commands": [
      {
        "command": "mathscript.transpile",
        "title": "MathScript: Transpile"
      }
    ]
  }
}
